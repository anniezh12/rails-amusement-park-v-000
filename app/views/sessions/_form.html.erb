
<%= form_for @user, url: {controller: 'sessions', action: 'create'} do |f| %>
    <!-- Select your name -->
    <%#=  collection_select :user, :id, User.all, :id,:name %>
    <p>
      <%= flash[:message] if flash[:message] %><br><br>
    <label>Name</label>
    <%= f.text_field :name %>
    <br>
    <label>Password</label>
    <%=f.password_field :password %>
    <br><br>
    <%= f.submit "Sign In" %>
 <% end %>
